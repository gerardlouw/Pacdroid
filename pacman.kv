#:kivy 1.0.9

<PacMan>:
    source: ''
    canvas:
        Color:
            rgb: 1, 1, 1
        Rectangle:
            source: self.source
            pos: self.pos
            size: self.size

<Ghost>:
    color: ''
    source: ''
    canvas:
        Color:
            rgb: 1, 1, 1
        Rectangle:
            source: self.source
            pos: self.pos
            size: self.size

<Wall>:
    canvas:
        Color:
            rgb: 0.2, 0.2, 0.2
        Rectangle:
            pos: self.pos
            size: self.size

<Pellet>:
    canvas:
        Color:
            rgb: 0, 1, 0
        Ellipse:
            pos: self.center_x-self.width*0.2, self.center_y-self.height*0.2
            size: self.width*0.4, self.height*0.4

<Pill>:
    canvas:
        Color:
            rgb: 1, 1, 1
        Ellipse:
            pos: self.center_x-self.width*0.25, self.center_y-self.height*0.25
            size: self.width*0.5, self.height*0.5

